{
  "title": "ArchManual",
  "plugins": [
    "sitemap",
    "sitemap-general",
    "github",
    "hide-element",
    "expandable-chapters",
    "back-to-top-button",
    "code",
    "splitter",
    "-lunr",
    "-search",
    "search-pro",
    "insert-logo",
    "custom-favicon",
    "pageview-count",
    "tbfed-pagefooter",
    "popup",
    "-sharing",
    "sharing-plus"
  ],
  "pluginsConfig": {
      "sitemap": {
        "hostname": "https://www.archmanual.com"
      },

      "sitemap-general": {
        "prefix": "https://www.archmanual.com"
      },
      "github": {
        "url":"https://github.com/yingqiangh/ArchManual"
      },
      "hide-element": {
        "elements": [".gitbook-link"]
      },
      "insert-logo": {
          "url": "/images/logo.png",
          "style": "background: none; max-height: 30px; min-height: 30px;"
      },
      "favicon": "images/fav.ico",
      "tbfed-pagefooter": {
          "copyright": "Copyright &copy archmanual.com 2024",
          "modify_label": "该文章修订时间：",
          "modify_format": "YYYY-MM-DD HH:mm:ss"
      },
      "sharing": {
          "all": [
              "douban",
              "facebook",
              "google",
              "instapaper",
              "linkedin",
              "twitter",
              "weibo",
              "messenger",
              "qq",
              "qzone",
              "viber",
              "whatsapp"
          ]
      }
  }
}
